{
  "ConnectionStrings": {
    "MongoDB": "mongodb://localhost:27017"
  },
  "MongoDbSettings": {
    "DatabaseName": "techsyslog"
  },
  "JwtSettings": {
    "SecretKey": "TechsysLog-Super-Secret-Key-Min-32-Characters-Long!",
    "Issuer": "TechsysLog",
    "Audience": "TechsysLogUsers",
    "ExpiresInHours": 24
  },
  "ViaCepSettings": {
    "BaseUrl": "https://viacep.com.br/ws",
    "TimeoutSeconds": 5
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:4200",
      "https://localhost:4200"
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}